	file = {Moulin et al. - 2020 - MRI of Reperfused Acute Myocardial Infarction Edem.pdf:C\:\\Users\\kevin\\Zotero\\storage\\MZPAETPW\\Moulin et al. - 2020 - MRI of Reperfused Acute Myocardial Infarction Edem.pdf:application/pdf},
}

