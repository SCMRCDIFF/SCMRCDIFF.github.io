	file = {Full Text PDF:C\:\\Users\\kevin\\Zotero\\storage\\LQEKNKAJ\\Gorodezky et al. - 2018 - Diffusion tensor cardiovascular magnetic resonance.pdf:application/pdf;Snapshot:C\:\\Users\\kevin\\Zotero\\storage\\9R22RDXW\\mrm.html:text/html},
