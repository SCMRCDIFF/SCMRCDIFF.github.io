	journal = {Magnetic Resonance in Medicine},
	author = {Moulin, Kévin and Croisille, Pierre and Viallon, Magalie and Verzhbinsky, Ilya A. and Perotti, Luigi E. and Ennis, Daniel B.},
	year = {2021},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/mrm.28724},
	keywords = {DENSE, myofiber strain, cDTI},
	pages = {277--292},
