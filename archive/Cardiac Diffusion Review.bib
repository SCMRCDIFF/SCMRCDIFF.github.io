	number = {1},
	urldate = {2019-03-17},
	journal = {Journal of Cardiovascular Magnetic Resonance},
	author = {Nguyen, Christopher and Fan, Zhaoyang and Xie, Yibin and Dawkins, James and Tseliou, Eleni and Bi, Xiaoming and Sharif, Behzad and Dharmakumar, Rohan and Marbán, Eduardo and Li, Debiao},
