	number = {1},
	urldate = {2019-03-17},
	journal = {Journal of Cardiovascular Magnetic Resonance},
	author = {Nguyen, Christopher and Lu, Minjie and Fan, Zhaoyang and Bi, Xiaoming and Kellman, Peter and Zhao, Shihua and Li, Debiao},
	month = dec,
	year = {2015},
	note = {Number: 1},
	file = {Nguyen et al. - 2015 - Contrast-free detection of myocardial fibrosis in .pdf:C\:\\Users\\kevin\\Zotero\\storage\\QPYMYSG2\\Nguyen et al. - 2015 - Contrast-free detection of myocardial fibrosis in .pdf:application/pdf},
